        function updateSlideCounter() {
            document.getElementById('currentSlide').textContent = currentSlide;
            document.getElementById('totalSlides').textContent = totalSlides;
        }

        function showSlide(n) {
            const slides = document.querySelectorAll('.slide');
            if (n > totalSlides) n = totalSlides;
            if (n < 1) n = 1;
            
            slides.forEach(slide => slide.classList.remove('active'));
            slides[n - 1].classList.add('active');
            
            currentSlide = n;
            updateSlideCounter();

            if (currentSlide === 18) {
                loadQuiz();
            } else if (currentSlide === 19) {
                showFinalResults();
            } else if (currentSlide === 20) {
                initializeLab();
            }

            if (currentSlide === 3 || currentSlide === 5) {
                setTimeout(createWaveAnimation, 100);
            }
            if (currentSlide === 8) {
                setTimeout(createTransverseWave, 100);
            }
            if (currentSlide === 10) {
                setTimeout(createStandingWave, 100);
            }
        }

        function nextSlide() {
            if (currentSlide === 18 && currentQuestion < quizQuestions.length && !answeredQuestions.has(currentQuestion)) {
                return;
            }
            showSlide(currentSlide + 1);
        }

        function prevSlide() {
            showSlide(currentSlide - 1);
        }

        function createWaveAnimation() {
            const containers = [
                document.getElementById('waveAnimation1'),
                document.getElementById('waveAnimation2')
            ];
            
            containers.forEach(container => {
                if (!container) return;
                container.innerHTML = '';
                
                const svg = document.createElementNS('http://www.w3.org/2000/svg', 'svg');
                svg.setAttribute('width', '100%');
                svg.setAttribute('height', '150');
                svg.setAttribute('viewBox', '0 0 800 150');
                
                const path = document.createElementNS('http://www.w3.org/2000/svg', 'path');
                let d = 'M 0 75';
                for (let x = 0; x <= 800; x += 10) {
                    const y = 75 + 30 * Math.sin((x / 800) * Math.PI * 4);
                    d += ` L ${x} ${y}`;
                }
                
                path.setAttribute('d', d);
                path.setAttribute('stroke', '#667eea');
                path.setAttribute('stroke-width', '3');
                path.setAttribute('fill', 'none');
                
                const animate = document.createElementNS('http://www.w3.org/2000/svg', 'animateTransform');
                animate.setAttribute('attributeName', 'transform');
                animate.setAttribute('type', 'translate');
                animate.setAttribute('from', '0 0');
                animate.setAttribute('to', '-200 0');
                animate.setAttribute('dur', '2s');
                animate.setAttribute('repeatCount', 'indefinite');
                
                path.appendChild(animate);
                svg.appendChild(path);
                container.appendChild(svg);
            });
        }

        function createTransverseWave() {
            const container = document.getElementById('waveAnimation3');
            if (!container) return;
            
            container.innerHTML = '';
            
            const svg = document.createElementNS('http://www.w3.org/2000/svg', 'svg');
            svg.setAttribute('width', '100%');
            svg.setAttribute('height', '150');
            svg.setAttribute('viewBox', '0 0 800 150');
            
            for (let i = 0; i < 10; i++) {
                const circle = document.createElementNS('http://www.w3.org/2000/svg', 'circle');
                circle.setAttribute('cx', i * 80 + 40);
                circle.setAttribute('cy', '75');
                circle.setAttribute('r', '8');
                circle.setAttribute('fill', '#667eea');
                
                const animate = document.createElementNS('http://www.w3.org/2000/svg', 'animate');
                animate.setAttribute('attributeName', 'cy');
                animate.setAttribute('values', '75;45;75;105;75');
                animate.setAttribute('dur', '2s');
                animate.setAttribute('begin', (i * 0.2) + 's');
                animate.setAttribute('repeatCount', 'indefinite');
                
                circle.appendChild(animate);
                svg.appendChild(circle);
            }
            
            container.appendChild(svg);
        }

        function createStandingWave() {
            const container = document.getElementById('standingWave');
            if (!container) return;
            
            container.innerHTML = '';
            
            const svg = document.createElementNS('http://www.w3.org/2000/svg', 'svg');
            svg.setAttribute('width', '100%');
            svg.setAttribute('height', '150');
            svg.setAttribute('viewBox', '0 0 800 150');
            
            const path = document.createElementNS('http://www.w3.org/2000/svg', 'path');
            
            function updatePath(t) {
                let d = 'M 0 75';
                for (let x = 0; x <= 800; x += 5) {
                    const amplitude = 30 * Math.sin((x / 800) * Math.PI * 3);
                    const y = 75 + amplitude * Math.sin(t);
                    d += ` L ${x} ${y}`;
                }
                path.setAttribute('d', d);
            }
            
            path.setAttribute('stroke', '#667eea');
            path.setAttribute('stroke-width', '3');
            path.setAttribute('fill', 'none');
            
            svg.appendChild(path);
            container.appendChild(svg);
            
            let t = 0;
            setInterval(() => {
                t += 0.1;
                updatePath(t);
            }, 50);
        }

        function loadQuiz() {
            if (currentQuestion >= quizQuestions.length) {
                return;
            }

            const container = document.getElementById('quizContainer');
            const question = quizQuestions[currentQuestion];
            
            let html = `<h3 style="color: #667eea; margin-bottom: 20px;">–°“±—Ä–∞“õ ${currentQuestion + 1}: ${question.question}</h3>`;
            
            question.options.forEach((option, index) => {
                html += `<div class="quiz-option" data-index="${index}">${option}</div>`;
            });
            
            container.innerHTML = html;
            
            const options = container.querySelectorAll('.quiz-option');
            options.forEach(option => {
                option.addEventListener('click', () => checkAnswer(option, question.correct));
            });
        }

        function checkAnswer(selectedOption, correctIndex) {
            if (answeredQuestions.has(currentQuestion)) {
                return;
            }

            answeredQuestions.add(currentQuestion);
            
            const container = document.getElementById('quizContainer');
            const options = container.querySelectorAll('.quiz-option');
            const feedback = document.getElementById('feedback');
            const nextBtn = document.getElementById('nextBtn');
            
            options.forEach(option => {
                option.style.pointerEvents = 'none';
            });
            
            const selectedIndex = parseInt(selectedOption.dataset.index);
            
            if (selectedIndex === correctIndex) {
                selectedOption.classList.add('correct');
                feedback.textContent = '‚úì –î“±—Ä—ã—Å!';
                feedback.className = 'quiz-feedback show correct';
                quizScore++;
            } else {
                selectedOption.classList.add('incorrect');
                options[correctIndex].classList.add('correct');
                feedback.textContent = '‚úó “ö–∞—Ç–µ! –î“±—Ä—ã—Å –∂–∞—É–∞–ø –∫”©—Ä—Å–µ—Ç—ñ–ª–¥—ñ.';
                feedback.className = 'quiz-feedback show incorrect';
            }
            
            document.getElementById('score').textContent = quizScore;
            
            if (currentQuestion < quizQuestions.length - 1) {
                nextBtn.classList.add('show');
            } else {
                nextBtn.textContent = '–ù”ô—Ç–∏–∂–µ–Ω—ñ –∫”©—Ä—É';
                nextBtn.classList.add('show');
            }
        }

        document.getElementById('nextBtn').addEventListener('click', () => {
            if (currentQuestion >= quizQuestions.length - 1) {
                showSlide(19);
                return;
            }
            
            currentQuestion++;
            document.getElementById('feedback').classList.remove('show');
            document.getElementById('nextBtn').classList.remove('show');
            document.getElementById('nextBtn').textContent = '–ö–µ–ª–µ—Å—ñ —Å“±—Ä–∞“õ';
            
            if (currentQuestion < quizQuestions.length) {
                loadQuiz();
            }
        });

        function showFinalResults() {
            document.getElementById('finalScore').textContent = quizScore;
            const resultMessage = document.getElementById('resultMessage');
            const resultEmoji = document.getElementById('resultEmoji');
            
            if (quizScore === 7) {
                resultMessage.innerHTML = '<p style="color: #4caf50; font-weight: bold;">”®—Ç–µ –∂–∞“õ—Å—ã! –°—ñ–∑ –±–∞—Ä–ª—ã“õ —Å“±—Ä–∞“õ—Ç–∞—Ä“ì–∞ –¥“±—Ä—ã—Å –∂–∞—É–∞–ø –±–µ—Ä–¥—ñ“£—ñ–∑! üåü</p>';
                resultEmoji.textContent = 'üèÜ';
            } else if (quizScore >= 5) {
                resultMessage.innerHTML = '<p style="color: #667eea; font-weight: bold;">–ñ–∞“õ—Å—ã –Ω”ô—Ç–∏–∂–µ! –ñ–∞–ª“ì–∞—Å—Ç—ã—Ä—ã“£—ã–∑! üëç</p>';
                resultEmoji.textContent = 'üéâ';
            } else if (quizScore >= 3) {
                resultMessage.innerHTML = '<p style="color: #ff9800; font-weight: bold;">“ö–∞–Ω–∞“ì–∞—Ç—Ç–∞–Ω–∞—Ä–ª—ã“õ. –ú–∞—Ç–µ—Ä–∏–∞–ª–¥—ã “õ–∞–π—Ç–∞–ª–∞“£—ã–∑! üìö</p>';
                resultEmoji.textContent = 'üìñ';
            } else {
                resultMessage.innerHTML = '<p style="color: #f44336; font-weight: bold;">–ú–∞—Ç–µ—Ä–∏–∞–ª–¥—ã “õ–∞–π—Ç–∞ –æ“õ—ã–ø —à—ã“ì—ã“£—ã–∑! üí™</p>';
                resultEmoji.textContent = 'üìù';
            }
        }

        document.getElementById('prevBtn').addEventListener('click', prevSlide);
        document.getElementById('nextBtn2').addEventListener('click', nextSlide);

        document.addEventListener('keydown', (e) => {
            if (e.key === 'ArrowLeft') prevSlide();
            if (e.key === 'ArrowRight') nextSlide();
        });

        async function onConfigChange(config) {
            const titleElement = document.getElementById('presentationTitle');
            const authorElement = document.getElementById('authorName');
            const introElement = document.getElementById('introText');
            
            if (titleElement) {
                titleElement.textContent = config.presentation_title || defaultConfig.presentation_title;
            }
            if (authorElement) {
                authorElement.textContent = config.author_name || defaultConfig.author_name;
            }
            if (introElement) {
                introElement.textContent = config.intro_text || defaultConfig.intro_text;
            }
        }

        if (window.elementSdk) {
            window.elementSdk.init({
                defaultConfig: defaultConfig,
                onConfigChange: onConfigChange,
                mapToCapabilities: (config) => ({
                    recolorables: [],
                    borderables: [],
                    fontEditable: undefined,
                    fontSizeable: undefined
                }),
                mapToEditPanelValues: (config) => new Map([
                    ["presentation_title", config.presentation_title || defaultConfig.presentation_title],
                    ["author_name", config.author_name || defaultConfig.author_name],
                    ["intro_text", config.intro_text || defaultConfig.intro_text]
                ])
            });
        }

        updateSlideCounter();
        createWaveAnimation();

        // –õ–∞–±–æ—Ä–∞—Ç–æ—Ä–∏—è–ª—ã“õ —Å–∏–º—É–ª—è—Ç–æ—Ä
        let currentSimulation = null;
        let animationId = null;
        let isPlaying = true;
        let amplitude = 30;
        let frequency = 1;

        function initializeLab() {
            const labCells = document.querySelectorAll('.lab-cell');
            labCells.forEach(cell => {
                cell.addEventListener('click', () => {
                    const type = cell.dataset.type;
                    selectSimulation(type, cell);
                });
            });

            // –ë–∞—Å“õ–∞—Ä—É –ø–∞–Ω–µ–ª—ñ
            const amplitudeSlider = document.getElementById('amplitudeSlider');
            const frequencySlider = document.getElementById('frequencySlider');
            const playPauseBtn = document.getElementById('playPauseBtn');
            const resetBtn = document.getElementById('resetBtn');

            if (amplitudeSlider) {
                amplitudeSlider.addEventListener('input', (e) => {
                    amplitude = parseInt(e.target.value);
                    document.getElementById('amplitudeValue').textContent = amplitude;
                });
            }

            if (frequencySlider) {
                frequencySlider.addEventListener('input', (e) => {
                    frequency = parseFloat(e.target.value);
                    document.getElementById('frequencyValue').textContent = frequency.toFixed(1);
                });
            }

            if (playPauseBtn) {
                playPauseBtn.addEventListener('click', togglePlayPause);
            }

            if (resetBtn) {
                resetBtn.addEventListener('click', resetSimulation);
            }
        }

        function selectSimulation(type, cellElement) {
            // –ë–∞—Ä–ª—ã“õ “±—è—à—ã“õ—Ç–∞—Ä–¥–∞–Ω active –∫–ª–∞—Å—ã–Ω –∞–ª—ã–ø —Ç–∞—Å—Ç–∞—É
            document.querySelectorAll('.lab-cell').forEach(cell => {
                cell.classList.remove('active');
            });
            
            // –¢–∞“£–¥–∞–ª“ì–∞–Ω “±—è—à—ã“õ—Ç—ã –±–µ–ª—Å–µ–Ω–¥—ñ –µ—Ç—É
            cellElement.classList.add('active');
            
            currentSimulation = type;
            document.getElementById('controlsPanel').style.display = 'block';
            
            startSimulation(type);
        }

        function startSimulation(type) {
            const simulationArea = document.getElementById('simulationArea');
            
            // –ê–Ω–∏–º–∞—Ü–∏—è–Ω—ã —Ç–æ“õ—Ç–∞—Ç—É
            if (animationId) {
                cancelAnimationFrame(animationId);
            }

            switch(type) {
                case 'longitudinal':
                    createLongitudinalSimulation(simulationArea);
                    break;
                case 'transverse':
                    createTransverseSimulation(simulationArea);
                    break;
                case 'standing':
                    createStandingWaveSimulation(simulationArea);
                    break;
                case 'water':
                    createWaterWaveSimulation(simulationArea);
                    break;
                case 'seismic':
                    createSeismicSimulation(simulationArea);
                    break;
                case 'interference':
                    createInterferenceSimulation(simulationArea);
                    break;
            }
        }

        function createLongitudinalSimulation(container) {
            container.innerHTML = `
                <div class="info-panel">
                    <h5>üîä –ë–æ–π–ª—ã“õ —Ç–æ–ª“õ—ã–Ω</h5>
                    <p><strong>–¢“Ø—Ä—ñ:</strong> –î—ã–±—ã—Å —Ç–æ–ª“õ—ã–Ω—ã</p>
                    <p><strong>–ë–∞“ì—ã—Ç:</strong> –ü–∞—Ä–∞–ª–ª–µ–ª—å</p>
                    <p><strong>–û—Ä—Ç–∞:</strong> –ê—É–∞</p>
                    <p><strong>–ñ—ã–ª–¥–∞–º–¥—ã“õ:</strong> ~340 –º/—Å</p>
                </div>
                <div class="particles-container" id="longitudinalContainer"></div>
            `;

            const particlesContainer = document.getElementById('longitudinalContainer');
            const particles = [];
            
            // –ë”©–ª—à–µ–∫—Ç–µ—Ä–¥—ñ –∂–∞—Å–∞—É
            for (let i = 0; i < 40; i++) {
                const particle = document.createElement('div');
                particle.className = 'wave-particle';
                particle.style.left = (i * 20) + 'px';
                particle.style.top = '90px';
                particlesContainer.appendChild(particle);
                particles.push({
                    element: particle,
                    originalX: i * 20,
                    index: i
                });
            }

            let time = 0;
            function animateLongitudinal() {
                if (!isPlaying) {
                    animationId = requestAnimationFrame(animateLongitudinal);
                    return;
                }

                particles.forEach(particle => {
                    const displacement = amplitude * 0.3 * Math.sin(frequency * time - particle.index * 0.3);
                    particle.element.style.left = (particle.originalX + displacement) + 'px';
                });

                time += 0.05;
                animationId = requestAnimationFrame(animateLongitudinal);
            }
            animateLongitudinal();
        }

        function createTransverseSimulation(container) {
            container.innerHTML = `
                <div class="info-panel">
                    <h5>üé∏ –ö”©–ª–¥–µ–Ω–µ“£ —Ç–æ–ª“õ—ã–Ω</h5>
                    <p><strong>–¢“Ø—Ä—ñ:</strong> –Ü—à–µ–∫ —Ç–µ—Ä–±–µ–ª—ñ—Å—ñ</p>
                    <p><strong>–ë–∞“ì—ã—Ç:</strong> –ü–µ—Ä–ø–µ–Ω–¥–∏–∫—É–ª—è—Ä</p>
                    <p><strong>–û—Ä—Ç–∞:</strong> “ö–∞—Ç—Ç—ã –¥–µ–Ω–µ</p>
                    <p><strong>–ñ—ã–ª–¥–∞–º–¥—ã“õ:</strong> –û—Ä—Ç–∞“ì–∞ –±–∞–π–ª–∞–Ω—ã—Å—Ç—ã</p>
                </div>
                <canvas class="wave-canvas" id="transverseCanvas"></canvas>
            `;

            const canvas = document.getElementById('transverseCanvas');
            const ctx = canvas.getContext('2d');
            canvas.width = canvas.offsetWidth;
            canvas.height = canvas.offsetHeight;

            let time = 0;
            function animateTransverse() {
                if (!isPlaying) {
                    animationId = requestAnimationFrame(animateTransverse);
                    return;
                }

                ctx.clearRect(0, 0, canvas.width, canvas.height);
                
                // –¢–æ–ª“õ—ã–Ω —Å—ã–∑—É
                ctx.beginPath();
                ctx.strokeStyle = '#667eea';
                ctx.lineWidth = 4;
                
                for (let x = 0; x < canvas.width; x += 2) {
                    const y = canvas.height / 2 + amplitude * Math.sin(frequency * time - x * 0.02);
                    if (x === 0) {
                        ctx.moveTo(x, y);
                    } else {
                        ctx.lineTo(x, y);
                    }
                }
                ctx.stroke();

                // –ë”©–ª—à–µ–∫—Ç–µ—Ä–¥—ñ –∫”©—Ä—Å–µ—Ç—É
                ctx.fillStyle = '#764ba2';
                for (let x = 0; x < canvas.width; x += 40) {
                    const y = canvas.height / 2 + amplitude * Math.sin(frequency * time - x * 0.02);
                    ctx.beginPath();
                    ctx.arc(x, y, 8, 0, Math.PI * 2);
                    ctx.fill();
                }

                time += 0.05;
                animationId = requestAnimationFrame(animateTransverse);
            }
            animateTransverse();
        }

        function createStandingWaveSimulation(container) {
            container.innerHTML = `
                <div class="info-panel">
                    <h5>üéª –¢“±—Ä“ì—ã–Ω —Ç–æ–ª“õ—ã–Ω</h5>
                    <p><strong>–¢“Ø—Ä—ñ:</strong> –†–µ–∑–æ–Ω–∞–Ω—Å</p>
                    <p><strong>–ï—Ä–µ–∫—à–µ–ª—ñ–∫:</strong> –¢“Ø–π—ñ–Ω–¥–µ—Ä –º–µ–Ω “õ–∞—Ä—ã–Ω–¥–∞—Ä</p>
                    <p><strong>“ö–æ–ª–¥–∞–Ω—É:</strong> –ú—É–∑—ã–∫–∞–ª—ã“õ –∞—Å–ø–∞–ø—Ç–∞—Ä</p>
                </div>
                <canvas class="wave-canvas" id="standingCanvas"></canvas>
            `;

            const canvas = document.getElementById('standingCanvas');
            const ctx = canvas.getContext('2d');
            canvas.width = canvas.offsetWidth;
            canvas.height = canvas.offsetHeight;

            let time = 0;
            function animateStanding() {
                if (!isPlaying) {
                    animationId = requestAnimationFrame(animateStanding);
                    return;
                }

                ctx.clearRect(0, 0, canvas.width, canvas.height);
                
                // –¢“±—Ä“ì—ã–Ω —Ç–æ–ª“õ—ã–Ω
                ctx.beginPath();
                ctx.strokeStyle = '#667eea';
                ctx.lineWidth = 4;
                
                for (let x = 0; x < canvas.width; x += 2) {
                    const standingAmplitude = amplitude * Math.sin(x * 0.02);
                    const y = canvas.height / 2 + standingAmplitude * Math.sin(frequency * time);
                    if (x === 0) {
                        ctx.moveTo(x, y);
                    } else {
                        ctx.lineTo(x, y);
                    }
                }
                ctx.stroke();

                // –¢“Ø–π—ñ–Ω–¥–µ—Ä–¥—ñ –∫”©—Ä—Å–µ—Ç—É
                ctx.fillStyle = '#f44336';
                for (let x = 0; x < canvas.width; x += 157) {
                    ctx.beginPath();
                    ctx.arc(x, canvas.height / 2, 6, 0, Math.PI * 2);
                    ctx.fill();
                }

                time += 0.05;
                animationId = requestAnimationFrame(animateStanding);
            }
            animateStanding();
        }

        function createWaterWaveSimulation(container) {
            container.innerHTML = `
                <div class="info-panel">
                    <h5>üåä –°—É —Ç–æ–ª“õ—ã–Ω—ã</h5>
                    <p><strong>–¢“Ø—Ä—ñ:</strong> –ë–µ—Ç—Ç—ñ–∫ —Ç–æ–ª“õ—ã–Ω</p>
                    <p><strong>–û—Ä—Ç–∞:</strong> –°“±–π—ã“õ –±–µ—Ç—ñ</p>
                    <p><strong>–ï—Ä–µ–∫—à–µ–ª—ñ–∫:</strong> –î”©“£–≥–µ–ª–µ–∫ —Ç–æ–ª“õ—ã–Ω–¥–∞—Ä</p>
                </div>
                <div class="ripple-container" id="rippleContainer"></div>
            `;

            const container2 = document.getElementById('rippleContainer');
            
            function createRipple() {
                if (!isPlaying) {
                    setTimeout(createRipple, 1000 / frequency);
                    return;
                }

                const ripple = document.createElement('div');
                ripple.className = 'ripple';
                ripple.style.left = '50%';
                ripple.style.top = '50%';
                ripple.style.transform = 'translate(-50%, -50%)';
                ripple.style.animationDuration = (3 / frequency) + 's';
                
                container2.appendChild(ripple);
                
                setTimeout(() => {
                    if (container2.contains(ripple)) {
                        container2.removeChild(ripple);
                    }
                }, 3000 / frequency);
                
                setTimeout(createRipple, 1000 / frequency);
            }
            createRipple();
        }

        function createSeismicSimulation(container) {
            container.innerHTML = `
                <div class="info-panel">
                    <h5>üåç –°–µ–π—Å–º–∏–∫–∞–ª—ã“õ —Ç–æ–ª“õ—ã–Ω</h5>
                    <p><strong>P-—Ç–æ–ª“õ—ã–Ω:</strong> –ë–æ–π–ª—ã“õ (~6-8 –∫–º/—Å)</p>
                    <p><strong>S-—Ç–æ–ª“õ—ã–Ω:</strong> –ö”©–ª–¥–µ–Ω–µ“£ (~3-5 –∫–º/—Å)</p>
                    <p><strong>–û—Ä—Ç–∞:</strong> –ñ–µ—Ä “õ—ã—Ä—Ç—ã—Å—ã</p>
                </div>
                <canvas class="wave-canvas" id="seismicCanvas"></canvas>
            `;

            const canvas = document.getElementById('seismicCanvas');
            const ctx = canvas.getContext('2d');
            canvas.width = canvas.offsetWidth;
            canvas.height = canvas.offsetHeight;

            let time = 0;
            function animateSeismic() {
                if (!isPlaying) {
                    animationId = requestAnimationFrame(animateSeismic);
                    return;
                }

                ctx.clearRect(0, 0, canvas.width, canvas.height);
                
                // P-—Ç–æ–ª“õ—ã–Ω (–∂—ã–ª–¥–∞–º)
                ctx.beginPath();
                ctx.strokeStyle = '#4caf50';
                ctx.lineWidth = 3;
                ctx.setLineDash([]);
                
                for (let x = 0; x < canvas.width; x += 2) {
                    const y = canvas.height / 3 + amplitude * 0.5 * Math.sin(frequency * time * 2 - x * 0.03);
                    if (x === 0) {
                        ctx.moveTo(x, y);
                    } else {
                        ctx.lineTo(x, y);
                    }
                }
                ctx.stroke();

                // S-—Ç–æ–ª“õ—ã–Ω (–±–∞—è—É)
                ctx.beginPath();
                ctx.strokeStyle = '#ff9800';
                ctx.lineWidth = 3;
                
                for (let x = 0; x < canvas.width; x += 2) {
                    const y = canvas.height * 2/3 + amplitude * 0.7 * Math.sin(frequency * time - x * 0.02);
                    if (x === 0) {
                        ctx.moveTo(x, y);
                    } else {
                        ctx.lineTo(x, y);
                    }
                }
                ctx.stroke();

                // –ë–µ–ª–≥—ñ–ª–µ—Ä
                ctx.fillStyle = '#4caf50';
                ctx.font = '14px Arial';
                ctx.fillText('P-—Ç–æ–ª“õ—ã–Ω (–∂—ã–ª–¥–∞–º)', 10, canvas.height / 3 - 10);
                
                ctx.fillStyle = '#ff9800';
                ctx.fillText('S-—Ç–æ–ª“õ—ã–Ω (–±–∞—è—É)', 10, canvas.height * 2/3 - 10);

                time += 0.05;
                animationId = requestAnimationFrame(animateSeismic);
            }
            animateSeismic();
        }

        function createInterferenceSimulation(container) {
            container.innerHTML = `
                <div class="info-panel">
                    <h5>‚ö° –ò–Ω—Ç–µ—Ä—Ñ–µ—Ä–µ–Ω—Ü–∏—è</h5>
                    <p><strong>–¢“Ø—Ä—ñ:</strong> –ï–∫—ñ —Ç–æ–ª“õ—ã–Ω–Ω—ã“£ “õ–æ—Å—ã–ª—É—ã</p>
                    <p><strong>–ù”ô—Ç–∏–∂–µ:</strong> –ö“Ø—à–µ–π—Ç—É –∂”ô–Ω–µ ”ô–ª—Å—ñ—Ä–µ—Ç—É</p>
                    <p><strong>“ö–æ–ª–¥–∞–Ω—É:</strong> –¢“±—Ä“ì—ã–Ω —Ç–æ–ª“õ—ã–Ω</p>
                </div>
                <canvas class="wave-canvas" id="interferenceCanvas"></canvas>
            `;

            const canvas = document.getElementById('interferenceCanvas');
            const ctx = canvas.getContext('2d');
            canvas.width = canvas.offsetWidth;
            canvas.height = canvas.offsetHeight;

            let time = 0;
            function animateInterference() {
                if (!isPlaying) {
                    animationId = requestAnimationFrame(animateInterference);
                    return;
                }

                ctx.clearRect(0, 0, canvas.width, canvas.height);
                
                // –ë—ñ—Ä—ñ–Ω—à—ñ —Ç–æ–ª“õ—ã–Ω
                ctx.beginPath();
                ctx.strokeStyle = 'rgba(102, 126, 234, 0.7)';
                ctx.lineWidth = 2;
                ctx.setLineDash([5, 5]);
                
                for (let x = 0; x < canvas.width; x += 2) {
                    const y = canvas.height / 2 + amplitude * 0.5 * Math.sin(frequency * time - x * 0.02);
                    if (x === 0) {
                        ctx.moveTo(x, y);
                    } else {
                        ctx.lineTo(x, y);
                    }
                }
                ctx.stroke();

                // –ï–∫—ñ–Ω—à—ñ —Ç–æ–ª“õ—ã–Ω
                ctx.beginPath();
                ctx.strokeStyle = 'rgba(118, 75, 162, 0.7)';
                ctx.lineWidth = 2;
                ctx.setLineDash([5, 5]);
                
                for (let x = 0; x < canvas.width; x += 2) {
                    const y = canvas.height / 2 + amplitude * 0.5 * Math.sin(frequency * time + x * 0.02);
                    if (x === 0) {
                        ctx.moveTo(x, y);
                    } else {
                        ctx.lineTo(x, y);
                    }
                }
                ctx.stroke();

                // –ù”ô—Ç–∏–∂–µ–ª—ñ–∫ —Ç–æ–ª“õ—ã–Ω
                ctx.beginPath();
                ctx.strokeStyle = '#667eea';
                ctx.lineWidth = 4;
                ctx.setLineDash([]);
                
                for (let x = 0; x < canvas.width; x += 2) {
                    const wave1 = amplitude * 0.5 * Math.sin(frequency * time - x * 0.02);
                    const wave2 = amplitude * 0.5 * Math.sin(frequency * time + x * 0.02);
                    const y = canvas.height / 2 + wave1 + wave2;
                    if (x === 0) {
                        ctx.moveTo(x, y);
                    } else {
                        ctx.lineTo(x, y);
                    }
                }
                ctx.stroke();

                time += 0.05;
                animationId = requestAnimationFrame(animateInterference);
            }
            animateInterference();
        }

        function togglePlayPause() {
            isPlaying = !isPlaying;
            const btn = document.getElementById('playPauseBtn');
            btn.textContent = isPlaying ? '‚è∏Ô∏è –¢–æ“õ—Ç–∞—Ç—É' : '‚ñ∂Ô∏è –ñ–∞–ª“ì–∞—Å—Ç—ã—Ä—É';
        }

        function resetSimulation() {
            if (currentSimulation) {
                startSimulation(currentSimulation);
            }
        }
    </script>
